<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title</title>

    <!-- Bootstrap CSS -->
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha3/dist/css/bootstrap.min.css" rel="stylesheet">
    <link rel="stylesheet" href="StyleSheet1.css" />
    <link rel="icon" href="HaDo.png" type="image/png">


    
</head>
<body>


    <header class="bg-dark text-white py-3">
        <nav class="container">
            <div class="d-flex align-items-center">
                <img src="HaDoTransparent.png" alt="HADO" width="100" class="me-3">
                <ul class="nav">
                    <li class="nav-item">
                        <a class="nav-link text-white" href="@Url.Action("Index", "Home")">Home</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link text-white" asp-controller="Projects" asp-action="Index">Projects</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link text-white" asp-controller="Skills" asp-action="Index">Skills</a>
                    </li>
                </ul>
            </div>
        </nav>
    </header>






    </header>
    <main class="container my-4">
        <div class="content-body">
            @RenderBody()
        </div>
    </main>
    <footer class="bg-dark text-white py-3 text-center">
        <p>Contact Info</p>
        <p>Email: <a href="mailto:Hadiyahkhan03@gmail.com">HadiyahKhan03@gmail.com</a></p>
        <p>Mobile: 343-988-0360</p>
    </footer>


    <!-- Bootstrap JS and Popper.js -->
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha3/dist/js/bootstrap.bundle.min.js"></script>

    @RenderSection("Scripts", required: false)

  
</body>
</html>
